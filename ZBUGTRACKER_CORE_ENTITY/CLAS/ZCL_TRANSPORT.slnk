 <method CLSNAME="ZCL_TRANSPORT" CMPNAME="ADD_BUG" VERSION="1" LANGU="E" DESCRIPT="Add Bug" EXPOSURE="2" STATE="1" EDITORDER="5 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20110308" CHANGEDBY="BCUSER" CHANGEDON="20110308" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="702" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZCL_TRANSPORT" CMPNAME="ADD_BUG" SCONAME="BUG" VERSION="1" LANGU="E" DESCRIPT="Bug Entity" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20110308" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZCL_BUG" PAROPTIONL="X"/>
  <source>METHOD add_bug.
  DATA: lv_str      TYPE zbt_bugs_str,
        lo_producto TYPE REF TO zcl_producto.

  lo_producto = bug-&gt;get_producto( ).
  lv_str-producto_id = lo_producto-&gt;get_id( ).
  lv_str-bug_id = bug-&gt;get_id( ).
  lv_str-oref = bug.

  READ TABLE bugs WITH KEY producto_id = lv_str-producto_id
                           bug_id      = lv_str-bug_id
                           TRANSPORTING NO FIELDS.
  IF NOT sy-subrc IS INITIAL.
    INSERT lv_str INTO TABLE bugs.
  ENDIF.

ENDMETHOD.</source>
 </method>
