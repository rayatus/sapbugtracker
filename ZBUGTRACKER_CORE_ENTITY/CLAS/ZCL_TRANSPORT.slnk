 <method CLSNAME="ZCL_ENTITY" CMPNAME="PREPARE_HASH_STRUCTURE" VERSION="0" EXPOSURE="0" STATE="0" EDITORDER="0 " DISPID="0 " CREATEDON="00000000" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
  <source>METHOD prepare_hash_structure.
  DATA: value TYPE LINE OF zbt_objectvalue_hash_calcul,
        id    TYPE zbt_transporter-transportreq.

  value = id = get_id( ).
  SHIFT value LEFT DELETING LEADING space.
  INSERT value INTO TABLE values[].
ENDMETHOD.</source>
 </method>
 <method CLSNAME="ZCL_TRANSPORT" CMPNAME="REMOVE_BUG" VERSION="1" LANGU="E" DESCRIPT="Remove Bug" EXPOSURE="2" STATE="1" EDITORDER="6 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20110309" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="702" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZCL_TRANSPORT" CMPNAME="REMOVE_BUG" SCONAME="BUG" VERSION="1" LANGU="E" DESCRIPT="Bug Entity" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20110309" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZCL_BUG"/>
  <source>METHOD remove_bug.
  DATA: lv_str      TYPE zbt_bugs_str,
        lo_producto TYPE REF TO zcl_producto.

  lo_producto = bug-&gt;get_producto( ).
  lv_str-producto_id = lo_producto-&gt;get_id( ).
  lv_str-bug_id = bug-&gt;get_id( ).

  DELETE bugs WHERE producto_id = lv_str-producto_id
                AND bug_id      = lv_str-bug_id.

ENDMETHOD.</source>
 </method>
