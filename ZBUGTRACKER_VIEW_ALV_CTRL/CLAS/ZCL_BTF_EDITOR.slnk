 <method CLSNAME="ZCL_BTF_EDITOR" CMPNAME="GET_CONTENT_AS_STRING" VERSION="1" LANGU="E" DESCRIPT="Get Content as String" EXPOSURE="2" STATE="1" EDITORDER="9 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101212" CHANGEDON="20101225" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZCL_BTF_EDITOR" CMPNAME="GET_CONTENT_AS_STRING" SCONAME="STRING" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101212" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="STRING"/>
  <source>METHOD get_content_as_string.
  DATA: l_xstring TYPE xstring.

  get_content( IMPORTING text = l_xstring ).
  CALL FUNCTION &apos;ECATT_CONV_XSTRING_TO_STRING&apos;
    EXPORTING
      im_xstring = l_xstring
    IMPORTING
      ex_string  = string.

ENDMETHOD.</source>
 </method>
