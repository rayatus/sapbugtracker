 <method CLSNAME="ZCL_BTF_EDITOR" CMPNAME="SET_CONTENT" VERSION="1" LANGU="E" DESCRIPT="Set Content" EXPOSURE="2" STATE="1" EDITORDER="8 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101031" CHANGEDBY="BCUSER" CHANGEDON="20101225" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZCL_BTF_EDITOR" CMPNAME="SET_CONTENT" SCONAME="TEXT" VERSION="1" LANGU="E" DESCRIPT="Document Text" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101031" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XSTRING"/>
  <parameter CLSNAME="ZCL_BTF_EDITOR" CMPNAME="SET_CONTENT" SCONAME="ENCODING" VERSION="1" LANGU="E" DESCRIPT="Document Coding (for example, &apos;utf-8&apos;)" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101031" CHANGEDBY="BCUSER" CHANGEDON="20101031" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING" PARVALUE="&apos;utf-8&apos;"/>
  <parameter CLSNAME="ZCL_BTF_EDITOR" CMPNAME="SET_CONTENT" SCONAME="CLEAN" VERSION="1" LANGU="E" DESCRIPT="Use Tidy &apos;0&apos; or &apos;1&apos;" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101031" CHANGEDBY="BCUSER" CHANGEDON="20110629" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="I" PARVALUE="0"/>
  <source>METHOD set_content.
  btf_doc-&gt;set_content(
    EXPORTING text     = text
              encoding = encoding
