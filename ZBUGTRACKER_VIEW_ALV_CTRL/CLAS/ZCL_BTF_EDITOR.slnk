  <parameter CLSNAME="ZCL_BTF_EDITOR" CMPNAME="GET_CONTENT" SCONAME="LANGUAGE" VERSION="1" LANGU="E" DESCRIPT="Language key" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101031" CHANGEDBY="BCUSER" CHANGEDON="20101031" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="TDSPRAS"/>
  <parameter CLSNAME="ZCL_BTF_EDITOR" CMPNAME="GET_CONTENT" SCONAME="ENCODING" VERSION="1" LANGU="E" DESCRIPT="Encoding" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101031" CHANGEDBY="BCUSER" CHANGEDON="20101031" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
  <parameter CLSNAME="ZCL_BTF_EDITOR" CMPNAME="GET_CONTENT" SCONAME="TEXT" VERSION="1" LANGU="E" DESCRIPT="Text" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20101031" CHANGEDBY="BCUSER" CHANGEDON="20101031" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="XSTRING" PARPREFERD="X"/>
  <source>METHOD get_content.
  editor-&gt;get_content( ).
